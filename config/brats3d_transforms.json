{
  "preprocessing": [
    {
      "name": "ToCanonical",
      "params": {}
    }
  ],

  "augmentations": [
    {
      "name": "RandomAffine",
      "params": {
        "scales": [0.9, 1.1],
        "degrees": [10, 10, 10],
        "translation": [5, 5, 5],
        "isotropic": false,
        "image_interpolation": "linear",
        "p": 0.4
      }
    },
    {
      "name": "RandomFlip",
      "params": {
        "axes": [0, 1, 2],
        "flip_probability": 0.4
      }
    },
    {
      "name": "RandomElasticDeformation",
      "params": {
        "num_control_points": 7,
        "max_displacement": 4.0,
        "locked_borders": 2,
        "image_interpolation": "linear",
        "p": 0.1
      }
    },
    {
      "name": "RandomBiasField",
      "params": {
        "coefficients": 0.5,
        "order": 3,
        "p": 0.1
      }
    },
    {
      "name": "RandomNoise",
      "params": {
        "mean": 0,
        "std": 0.01,
        "p": 0.1
      }
    },
    {
      "name": "RandomGamma",
      "params": {
        "log_gamma": [-0.15, 0.15],
        "p": 0.1
      }
    },
    {
      "name": "RandomBlur",
      "params": {
        "std": [0.5, 1.0],
        "p": 0.1
      }
    }
  ]

}
